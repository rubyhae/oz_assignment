      "source": [
        "arr1 = [[1,2],[2,3]]\n",
        "\n",
        "arr2 = [[3,4],[5,6]]\n",
        "\n",
        "result = [[c + d for c, d in zip(a, b)] for a, b in zip(arr1, arr2)]\n",
        "\n",
        "print(result)\n",
        "\n",
        "print(zip(arr1, arr2))\n",
        "print(list(zip(arr1, arr2)))"
      ]
